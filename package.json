{
	"name": "@heiwa4126/md2drupal",
	"version": "0.0.4",
	"type": "module",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"bin": {
		"md2drupal": "dist/esm/index.js"
	},
	"scripts": {
		"format": "biome format --write . && dprint fmt && textlint --fix \"**/*.md\"",
		"check": "biome check --write . && dprint check",
		"ex1": "node dist/dev/index.js testdata/test1.md",
		"s0": "node dist/dev/s0.js testdata/test1.md",
		"phi3": "node dist/dev/index.js ~/works/some-notes2/localLLM/using-phi3-locally-blog.md",
		"phi31": "node dist/dev/index.js ~/works/some-notes2/localLLM/using-phi3-locally-blog1.md",
		"phi32": "node dist/dev/index.js ~/works/some-notes2/localLLM/using-phi3-locally-blog2.md",
		"blog2": "node dist/dev/index.js ~/works/some-notes2/LLM/aws-bedrock-minimum-example-blog2.md",
		"cdn": "node dist/dev/index.js ~/works/some-notes2/javascript-cdn-blog.md",
		"esm": "node dist/dev/index.js ~/works/some-notes2/esm-on-browser-blog.md",
		"rsbuild": "node dist/dev/index.js ~/works/some-notes2/rsbuild-blog.md",
		"sso": "node dist/dev/index.js ~/works/some-notes2/IAM-Identity-Center-blog.md",
		"awsgpu": "node dist/dev/index.js ~/works/some-notes2/aws-gpu-blog.md",
		"a1": "node dist/dev/index.js ~/works/some-notes2/uv-tips-blog.md",
		"a2": "node dist/dev/index.js ~/works/some-notes2/aws-slr-blog.md",
		"a3": "node dist/dev/index.js ~/works/some-notes2/gpt-4o-mini-amazon-nova-lite-blog.md",
		"a4": "node dist/dev/index.js ~/works/some-notes2/effortless-esm-cdn-importmap.md",
		"a5": "node dist/dev/index.js ~/works/some-notes2/github-pages-custom-domain-note.md",
		"a6": "node dist/dev/index.js ~/works/some-notes2/cloudflare-pages-blog.md",
		"a7": "node dist/dev/index.js ~/works/some-notes2/journalctl-vacuum-blog.md",
		"a8": "node dist/dev/index.js ~/works/some-notes2/cloudflare-functions-blog.md",
		"a9": "node dist/dev/index.js ~/works/some-notes2/sms-blog.md",
		"b1": "node dist/dev/index.js ~/works/some-notes2/emv-contactless-blog.md",
		"watch": "tsc --watch",
		"test": "echo \"Error: no test specified\" && exit 1",
		"build:esm": "rm -rf dist/esm && tsc -p tsconfig.esm.json",
		"build:cjs": "rm -rf dist/cjs && tsc -p tsconfig.cjs.json",
		"build": "pnpm run build:esm && pnpm run build:cjs",
		"pack": "pnpm run format && pnpm run build && pnpm pack && mv *.tgz ../",
		"trivy-check": "trivy config . && trivy fs .",
		"trivy-license": "trivy fs --scanners license --severity HIGH,CRITICAL .",
		"trivy-sbom": "trivy fs --format spdx-json --output project-sbom.json ."
	},
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/heiwa4126/md2drupal.git"
	},
