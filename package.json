{
	"name": "@heiwa4126/md2drupal",
	"version": "0.0.7-alpha.1",
	"type": "module",
	"main": "./dist/converter1.js",
	"types": "./dist/converter1.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/converter1.d.ts",
				"default": "./dist/converter1.js"
			},
			"require": {
				"types": "./dist/converter1.d.ts",
				"default": "./dist/converter1.cjs"
			}
		}
	},
	"bin": {
		"md2drupal": "dist/index.js"
	},
	"scripts": {
		"cli": "tsx src/index.ts",
		"build": "tsup",
		"test": "vitest run",
		"test:run": "vitest run",
		"test:watch": "vitest",
		"clean": "rm -rf dist",
		"prepublishOnly": "pnpm run lint && pnpm run test && pnpm run clean && pnpm run build && pnpm run smoke-test",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"format:full": "pnpm pkg fix && biome format --write . && dprint fmt && textlint --fix \"**/*.md\"",
		"esm-smoke1": "node dist/index.js -h",
		"smoke-test": "pnpm run esm-smoke1 && node smoke_test/ex1.mjs  && node smoke_test/ex1.cjs && tsx smoke_test/ex1.ts",
		"prepack": "clean-publish-scripts -c",
		"postpack": "clean-publish-scripts -r",
		"trivy": "trivy config . && trivy fs --exit-code 1 --severity HIGH,CRITICAL .",
		"sbom": "trivy fs --scanners vuln --format spdx-json --output sbom.spdx.json .",
		"osv-scan": "osv-scanner -r .",
		"cloc": "cloc . --exclude-list-file=.gitignore",
		"ex1": "node dist/index.js testdata/test1.md",
		"ex2": "node dist/index.js testdata/test2.md",
		"phi3": "node dist/index.js ~/works/some-notes2/localLLM/using-phi3-locally-blog.md",
		"phi31": "node dist/index.js ~/works/some-notes2/localLLM/using-phi3-locally-blog1.md",
		"phi32": "node dist/index.js ~/works/some-notes2/localLLM/using-phi3-locally-blog2.md",
		"blog2": "node dist/index.js ~/works/some-notes2/LLM/aws-bedrock-minimum-example-blog2.md",
		"cdn": "node dist/index.js ~/works/some-notes2/javascript-cdn-blog.md",
		"esm": "node dist/index.js ~/works/some-notes2/esm-on-browser-blog.md",
		"rsbuild": "node dist/index.js ~/works/some-notes2/rsbuild-blog.md",
		"sso": "node dist/index.js ~/works/some-notes2/IAM-Identity-Center-blog.md",
		"awsgpu": "node dist/index.js ~/works/some-notes2/aws-gpu-blog.md",
		"a1": "node dist/index.js ~/works/some-notes2/uv-tips-blog.md",
		"a2": "node dist/index.js ~/works/some-notes2/aws-slr-blog.md",
		"a3": "node dist/index.js ~/works/some-notes2/gpt-4o-mini-amazon-nova-lite-blog.md",
		"a4": "node dist/index.js ~/works/some-notes2/effortless-esm-cdn-importmap.md",
		"a5": "node dist/index.js ~/works/some-notes2/github-pages-custom-domain-note.md",
		"a6": "node dist/index.js ~/works/some-notes2/cloudflare-pages-blog.md",
		"a7": "node dist/index.js ~/works/some-notes2/journalctl-vacuum-blog.md",
		"a8": "node dist/index.js ~/works/some-notes2/cloudflare-functions-blog.md",
		"a9": "node dist/index.js ~/works/some-notes2/sms-blog.md",
		"b1": "node dist/index.js ~/works/some-notes2/emv-contactless-blog.md",
		"b2": "node dist/index.js ~/works/some-notes2/second-argument-of-annotated-blog.md",
		"b3": "node dist/index.js ~/works/some-notes2/minimum-mcp-blog.md",
		"b4": "node dist/index.js ~/works/some-notes2/bound-method-blog.md",
		"b5": "node dist/index.js ~/works/some-notes2/langchain-structured-output-blog.md",
		"b6": "node dist/index.js ~/works/some-notes2/ai-agent-101-blog.md",
		"b7": "node dist/index.js ~/works/some-notes2/keyless-signing-blog.md",
		"b8": "node dist/index.js ~/works/some-notes2/postgres-nondeterministic-icu-collation-like-blog.md",
		"s1": "node dist/index.js ~/works/some-notes2/sigstore-part1a-blog.md",
		"s2": "node dist/index.js ~/works/some-notes2/sigstore-part2a-blog.md",
		"s3": "node dist/index.js ~/works/some-notes2/sigstore-part3a-blog.md"
	},
	"keywords": [
		"markdown",
		"drupal",
		"html",
		"converter",
		"unified",
		"remark",
		"rehype",
		"cli"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/heiwa4126/md2drupal.git"
	},
	"bugs": {
		"url": "https://github.com/heiwa4126/md2drupal/issues"
	},
	"homepage": "https://github.com/heiwa4126/md2drupal#readme",
	"license": "MIT",
	"description": "Convert Markdown to Drupal-compatible HTML using the Unified ecosystem (remark/rehype)",
	"files": [
		"dist/**/*.js",
		"dist/**/*.cjs",
		"dist/**/*.d.ts",
		"!dist/**/*.map"
	],
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@heiwa4126/clean-publish-scripts": "^0.0.2",
		"@types/escape-html": "^1.0.4",
		"@types/hast": "^3.0.4",
		"@types/mdast": "^4.0.4",
		"@types/node": "^25.0.0",
		"@types/unist": "^3.0.3",
		"linkedom": "^0.18.12",
		"tsup": "^8.5.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.15"
	},
	"dependencies": {
		"commander": "^14.0.2",
		"escape-html": "^1.0.3",
		"rehype-stringify": "^10.0.1",
		"remark-extract-frontmatter": "^3.2.0",
		"remark-frontmatter": "^5.0.0",
		"remark-gfm": "^4.0.1",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"unified": "^11.0.5",
		"unist-util-visit": "^5.0.0",
		"yaml": "^2.8.2"
	},
	"pnpm": {
		"overrides": {
			"chokidar": "4.0.1"
		}
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
