{
	"name": "@heiwa4126/md2drupal",
	"version": "0.0.3",
	"type": "module",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"bin": {
		"md2drupal": "dist/esm/index.js"
	},
	"scripts": {
		"ex1": "node dist/dev/index.js testdata/test1.md",
		"s0": "node dist/dev/s0.js testdata/test1.md",
		"phi3": "node dist/dev/index.js ~/works/some-notes2/localLLM/using-phi3-locally-blog.md",
		"phi31": "node dist/dev/index.js ~/works/some-notes2/localLLM/using-phi3-locally-blog1.md",
		"phi32": "node dist/dev/index.js ~/works/some-notes2/localLLM/using-phi3-locally-blog2.md",
		"blog2": "node dist/dev/index.js ~/works/some-notes2/LLM/aws-bedrock-minimum-example-blog2.md",
		"watch": "tsc --watch",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"check": "biome check --write .",
		"test": "echo \"Error: no test specified\" && exit 1",
		"build:esm": "rm -rf dist/esm && tsc -p tsconfig.esm.json",
		"build:cjs": "rm -rf dist/cjs && tsc -p tsconfig.cjs.json",
		"build": "pnpm run build:esm && pnpm run build:cjs",
		"pack": "pnpm run format && pnpm run build && pnpm pack && mv *.tgz ../",
		"trivy-check": "trivy config . && trivy fs .",
		"trivy-license": "trivy fs --scanners license --severity HIGH,CRITICAL .",
		"trivy-sbom": "trivy fs --format spdx-json --output project-sbom.json ."
	},
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/heiwa4126/md2drupal.git"
	},
	"bugs": {
		"url": "https://github.com/heiwa4126/md2drupal/issues"
	},
	"homepage": "https://github.com/heiwa4126/md2drupal#readme",
	"license": "MIT",
	"description": "",
	"files": [
		"package.json",
		"README.md",
		"LICENSE",
		"dist/esm",
		"dist/cjs",
		"!**/__tests__",
		"!**/*.test.*",
		"!**/*.js.map"
	],
	"packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a",
	"devDependencies": {
		"@biomejs/biome": "1.8.2",
		"@types/hast": "^3.0.4",
		"@types/minimist": "^1.2.5",
		"@types/node": "^20.14.13",
		"@types/unist": "^3.0.2",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"minimist": "^1.2.8",
		"rehype-stringify": "^10.0.0",
		"remark-gfm": "^4.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.0",
		"unified": "^11.0.5",
		"unist-util-visit": "^5.0.0"
	}
}
